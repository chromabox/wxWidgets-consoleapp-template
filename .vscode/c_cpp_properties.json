{
    "configurations": [
        {
            "name": "main",
            "compilerPath": "/usr/bin/gcc",
            "defines": [
                "DEBUG",
                "_FILE_OFFSET_BITS=64",
                "WXUSINGDLL",
                "wxUSE_GUI=0"
            ],
            "compilerArgs": [
                "-std=gnu++20"
            ],
            "includePath": [
                "/usr/include/wx-3.0",
                "/usr/lib/x86_64-linux-gnu/wx/include/base-unicode-3.0",
                "./include"
            ],
            "cppStandard": "${default}",
            "intelliSenseMode":"linux-gcc-x64",
            "browse": {
                "databaseFilename": "${workspaceFolder}/.vscode/browse.out",
                "limitSymbolsToIncludedHeaders": false,
                "path":["${workspaceFolder}"]
            }
		}
	],
    "version": 4
}
